{
  "useTabs": true,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 120,
  "proseWrap": "always",
  "plugins": [
    "prettier-plugin-svelte",
    "prettier-plugin-tailwindcss"
  ],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
